<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/Site.css")
    @*Scripts.Render("~/bundles/modernizr")*@
</head>
<body>
    <nav class="navbar bg-dark navbar-expand-lg">
        <div class="container">
            <a class="navbar-brand" href="./index.html">
                <img src="~/Content/img/layered-waves-haikei (1).png" alt="" height="85" />
            </a>
            <a class="navbar-brand" href="./index.html">
                <div class="navbar-text">
                    <span class="fw-bold text-white fs-2">GIG Ticketera</span>
                </div>
            </a>
            <button class="navbar-toggler navbar-dark border-0"
                    type="button"
                    data-bs-toggle="collapse"
                    data-bs-target="#navbarNav"
                    aria-controls="navbarNav"
                    aria-expanded="false"
                    aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse justify-content-center"
                 id="navbarNav">
                <ul class="navbar-nav">
                    <li class="nav-item">
                        <a class="nav-link text-white" href="./index.html">Inicio</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="./prueba.html">Seguimiento de Tickets</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link text-white" href="">Reportar Incidencia</a>
                    </li>
                </ul>
            </div>
            <div class="d-flex align-items-center">
                <button type="button" class="btn btn-danger px-3 me-2">Login</button><div class="container">
                <button type="submit" class="btn btn-primary text-white me-3" onclick="@Url.Action("Register", "Account")'">Sign up</button>
                </div>
            </div>
        </div>
    </nav>
    @RenderBody()

    @*

    *@

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/complementos")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
<script src="~/Scripts/jquery.validate.min.js"></script>
<script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
